<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Name</th>
      <th>Age</th>
      <th>Address</th>
      <th>Religion</th>
      <th>Jobs</th>
      <th>Action</th>

    </tr>
    </thead>
    <tbody>

      <% for(let i =0 ; i<category.length; i++){ %>
        <tr>
          <td>
            <%= i + 1 %>
            
          </td>
          <td><%= category[i].name %> </td>
          <td><%= category[i].age %></td>
          <td><%= category[i].address %></td>
          <td><%= category[i].religion %></td>
          <td><%= category[i].job %></td>
          <td>
            <form action="/category/delete/<%= category[i]._id%>?_method=DELETE" method="POST">
              <a href="/category/edit/<%= category[i]._id %>"class="btn btn-outline-info btn-sm">
                <i class="fas fa-edit"></i>
                  Edit
              </a>
              <button class="btn btn-outline-danger btn-sm"><i class="far fa-trash-alt"></i>Delete</button>
            </form>
          </td>
        </tr>
       <% } %> 

      
    </tbody>

  </table>                
  
